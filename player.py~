import card

class Player:
    def __init__(self, name, stack, position, cards, bet):
        self.name = name
        self.stack = stack
        self.cards = cards
        self.position = position
        self.bet = bet



    self.cards = [];
    
    def getCards(self, cardsDealt):
        for card in range(0, len(cardsDealt)):
            self.cards.append(card);
